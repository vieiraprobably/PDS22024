{
  "configurations": [
    {
      "name": "windows-gcc-x86",
      "includePath": [
        "${workspaceFolder}/**"
      ],
      "compilerPath": "C:/MinGW/bin/gcc.exe",
      "cStandard": "${default}",
      "cppStandard": "${default}",
      "intelliSenseMode": "windows-gcc-x86",
      "compilerArgs": [],
      "windowsSdkVersion": "10.0.22000.0",
      "defines": [
        "\"includePath\": [",
        "    \"C:/msys64/mingw64/include/**\",",
        "    \"C:/MinGW/include/**\",",
        "    \"C:/Program Files/LLVM/include/**\"",
        "],",
        "\"intelliSenseMode\": \"windows-gcc-x64\""
      ]
    },
    {
      "name": "macOS (Clang)",
      "includePath": [
        "${workspaceFolder}/**",
        "/usr/local/include/**"
      ],
      "compilerPath": "/usr/local/bin/clang",
      "cStandard": "${default}",
      "cppStandard": "${default}",
      "intelliSenseMode": "macos-clang-x64",
      "compilerArgs": []
    },
    {
      "name": "Linux (GCC)",
      "includePath": [
        "${workspaceFolder}/**",
        "/usr/include/**"
      ],
      "compilerPath": "/usr/bin/gcc",
      "cStandard": "${default}",
      "cppStandard": "${default}",
      "intelliSenseMode": "linux-gcc-x64",
      "compilerArgs": []
    }
  ],
  "version": 4
}